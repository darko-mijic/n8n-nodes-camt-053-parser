{
  "name": "n8n-nodes-camt-053-parser",
  "version": "0.1.0",
  "description": "n8n node for parsing ISO 20022 camt.053 messages to JSON",
  "keywords": [
    "n8n-community-node-package",
    "n8n-node",
    "n8n",
    "camt.053",
    "iso20022",
    "camt",
    "bank statement",
    "bank statement parser"
  ],
  "license": "MIT",
  "homepage": "https://github.com/darkomijic/n8n-nodes-camt-053-parser",
  "author": {
    "name": "Darko Mijic",
    "email": "darko.mijic@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/darkomijic/n8n-nodes-camt-053-parser.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/Camt053Parser/Camt053Parser.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "camt-parser": "^2.0.0"
  }
}
